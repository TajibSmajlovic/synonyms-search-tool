{
  "extends": ["react-app", "react-app/jest"],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn",
    "array-callback-return": "warn",
    "react-hooks/exhaustive-deps": "warn",
    "no-empty-function": "error",
    "no-duplicate-imports": "error",
    "no-nested-ternary": "error"
  }
}
